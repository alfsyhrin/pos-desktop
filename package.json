{
  "name": "pos-desktop",
  "version": "1.0.0",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-builder --win --publish never",
    "build:mac": "electron-builder --mac --publish never",
    "build": "electron-builder --publish never"
  },
  "build": {
    "appId": "com.pipos.myapp",
    "productName": "PiPos",
    "win": {
      "target": "nsis",
      "icon": "build/Pipos.ico"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.business"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "3": "^2.1.0",
    "@node-escpos/core": "^0.6.0",
    "@node-escpos/usb-adapter": "^0.3.1",
    "better-sqlite3": "^12.5.0",
    "electron-pos-printer": "^1.3.8",
    "iconv-lite": "^0.7.1",
    "node-thermal-printer": "^4.5.0",
    "sqlite": "^5.1.1",
    "xlsx": "^0.18.5"
  },
  "description": "Aplikasi PiPos Desktop",
  "author": "PT. Kamunara Group International"
}
